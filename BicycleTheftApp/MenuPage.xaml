<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BicycleTheftApp.MenuPage">
    <Grid>
    
        <Frame BackgroundColor="#B6C5CA"></Frame>
        <Image Source="menulogo.png" HorizontalOptions="Center" VerticalOptions="Center" TranslationY="-100" Scale="0.9"></Image>
        <ImageButton x:Name="bunpuzu" Source="menubutton1.png" Clicked="OnClicked" Pressed="ImageButton_Pressed" HorizontalOptions="Center" VerticalOptions="Center" Scale="0.5" TranslationY="50"></ImageButton>

        <ImageButton x:Name="secu" Source="menubutton2.png" Clicked="num_Clicked" Pressed="secu_Pressed" HorizontalOptions="Center" VerticalOptions="Center" Scale="0.5" TranslationY="100"></ImageButton>


    </Grid>
</ContentPage>